---
id: SANDETEL
version: "1.0"
type: agreement
context:
  provider: Provider
  consumer: Sandetel - Andalusian administration
  validity:
    timeZone: Europe/Madrid
    initial: '2016-07-13T00:00:00.000Z'
  definitions:
    schemas:
      MonthlyBillPenaltyPercentage:
        description: percentage of the fee to pay for a month of the service. 
        type: integer
        unit: '%'
      TotalBudget:
        description: total budget to pay to the provider for the service. 
        type: double
        unit: 'euros'
    scopes: {}
terms:
  pricing:
    MonthlyBill:
      period: monthly
      initial: '2016-05-12T10:35:36.000Z'
      penalties:
        - over:
            MonthlyBillPenaltyPercentage:
              $ref: '#/context/definitions/schemas/MonthlyBillPenaltyPercentage'
  metrics:
    NormalInterventionsSolved:
      schema:
        description: Percentage of non-urgent interventions solved in a month
        type: integer
        unit: '%'
      computer: >-
        http://Sandetel/NormalInterventionsSolved/
    UrgentInterventionsSolved:
      schema:
        description: Percentage of urgent interventions solved in a month
        type: integer
        unit: '%'
      computer: >-
        http://Sandetel/UrgentInterventionsSolved/
  guarantees:
    - id: NormalInterventions_GT
      scope: {}
      of:
        - objective: 'NormalInterventionsSolved > 90'
          with:
            NormalInterventionsSolved: {}
          window:
            type: static
            period: monthly
            initial: '2016-07-13T00:00:00.000Z'
          scope: {}
          penalties:
            - over:
                MonthlyBillPenaltyPercentage:
                  $ref: '#/context/definitions/schemas/MonthlyBillPenaltyPercentage'
              of:
                - value: '-1' 
                  condition: '(NormalInterventionsSolved >= 94) AND (NormalInterventionsSolved < 95)'
                - value: '-2' 
                  condition: '(NormalInterventionsSolved >= 93) AND (NormalInterventionsSolved < 94)'
                - value: '-3' 
                  condition: '(NormalInterventionsSolved >= 92) AND (NormalInterventionsSolved < 93)' 
                - value: '-4' 
                  condition: '(NormalInterventionsSolved >= 91) AND (NormalInterventionsSolved < 92)' 
                - value: '-5' 
                  condition: '(NormalInterventionsSolved >= 90) AND (NormalInterventionsSolved < 91)' 
                - value: '-10' 
                  condition: 'NormalInterventionsSolved < 90' 
              upTo:
                - value: 'MonthlyBill * 0.25'
                - value: 'TotalBudget * 0.10'
    - id: UrgentInterventions_GT
      scope: {}
      of:
        - objective: 'UrgentInterventionsSolved > 95'
          with:
            UrgentInterventionsSolved: {}
          window:
            type: static
            period: monthly
            initial: '2016-07-13T00:00:00.000Z'
          scope: {}
          penalties:
            - over:
                MonthlyBillPenaltyPercentage:
                  $ref: '#/context/definitions/schemas/MonthlyBillPenaltyPercentage'
              of:
                - value: '-1' 
                  condition: '(UrgentInterventionsSolved >= 94) AND (UrgentInterventionsSolved < 95)'
                - value: '-2' 
                  condition: '(UrgentInterventionsSolved >= 93) AND (UrgentInterventionsSolved < 94)'
                - value: '-3' 
                  condition: '(UrgentInterventionsSolved >= 92) AND (UrgentInterventionsSolved < 93)' 
                - value: '-4' 
                  condition: '(UrgentInterventionsSolved >= 91) AND (UrgentInterventionsSolved < 92)' 
                - value: '-5' 
                  condition: '(UrgentInterventionsSolved >= 90) AND (UrgentInterventionsSolved < 91)' 
                - value: '-10' 
                  condition: 'UrgentInterventionsSolved < 90' 
              upTo:
                - value: 'MonthlyBill * 0.25'
                - value: 'TotalBudget * 0.10'